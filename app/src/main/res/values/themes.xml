<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.AppBasic" parent="AppTheme">
        <item name="textNormal">@style/TextNormal</item>
        <item name="textNormalBlack">@style/TextNormal.Black</item>
        <item name="textNormalMuted">@style/TextNormal.Muted</item>
        <item name="textTitle">@style/TextTitle</item>
        <item name="textTitleMuted">@style/TextTitle.Muted</item>
        <item name="tintAccent">@style/TintAccent</item>
        <item name="tintNeutralDark">@style/TintNeutralDark</item>
        <item name="splashScreen">@style/SplashScreen</item>
        <item name="navHeader">@style/NavHeader.Green</item>
        <item name="itemListImageButton">@style/ItemListImageButton</item>
    </style>

    <style name="Theme.AppBasic.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.AppBasic.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AppBasic.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>